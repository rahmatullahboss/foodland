{
  "name": "dc-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:cf": "npx opennextjs-cloudflare build",
    "start": "next start",
    "lint": "eslint",
    "preview": "wrangler dev",
    "deploy": "npm run build:cf && wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply dc-store-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply dc-store-db --remote",
    "db:seed:local": "wrangler d1 execute dc-store-db --local --file=./drizzle/seed.sql",
    "db:seed:remote": "wrangler d1 execute dc-store-db --remote --file=./drizzle/seed.sql",
    "db:studio:local": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "cf:login": "wrangler login",
    "cf:d1:create": "wrangler d1 create dc-store-db"
  },
  "dependencies": {
